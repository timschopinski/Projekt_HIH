<!DOCTYPE journeys [
        <!ELEMENT journeys (logo|slides|table|info|form)*>
        <!ELEMENT logo (text)*>
        <!ELEMENT text (#PCDATA)>
        <!ELEMENT slides (slide)*>
        <!ELEMENT slide (image)*>
        <!ELEMENT image (#PCDATA)>
        <!ATTLIST image
                source CDATA #REQUIRED>
        <!ELEMENT table (country)*>
        <!ELEMENT country (title|place|points)*>
        <!ELEMENT title (#PCDATA)>
        <!ELEMENT place (#PCDATA)>
        <!ELEMENT points (#PCDATA)>
        <!ELEMENT info (hobby|media|kontakt)*>
        <!ELEMENT hobby (descr)*>
        <!ELEMENT descr (#PCDATA)>
        <!ELEMENT media (description|links)*>
        <!ELEMENT description (text)*>
        <!ATTLIST description
                kind CDATA #REQUIRED>
        <!ELEMENT links (link)*>
        <!ELEMENT link (#PCDATA)>
        <!ATTLIST link
                href CDATA #REQUIRED>
        <!ELEMENT kontakt (phone|email)*>
        <!ELEMENT phone (#PCDATA)>
        <!ELEMENT email (#PCDATA)>
        <!ELEMENT form (person)*>
        <!ELEMENT person (name|surname|gender|age)*>
        <!ATTLIST person
                id CDATA #REQUIRED>
        <!ELEMENT name (#PCDATA)>
        <!ELEMENT surname (#PCDATA)>
        <!ELEMENT gender (#PCDATA)>
        <!ELEMENT age (#PCDATA)>
        ]>
